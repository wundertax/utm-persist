{
  "parser": "babel-eslint",
  "rules": {
    "curly": [2],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "comma-dangle": [0],
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "none",
      "ignoreRestSiblings": true
    }],
    "no-console": [0],
    "object-curly-spacing": [2, "always"],
    "keyword-spacing": ["error"],
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error",
    "jest/valid-expect": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "jest"
  ]
}
