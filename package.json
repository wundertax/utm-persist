{
  "name": "@wundertax/utm-persist",
  "version": "1.0.0",
  "description": "Persist UTM parameters to Local Storage",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundertax/utm_persist.git"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel ./src -d ./dist --presets env",
    "build:readme": "toctoc README.md -w",
    "init": "mkdir dist",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "prebuild": "npm run clean && npm run init",
    "publish-to-npm": "npm run build:readme && npm run build && npm publish --access public",
    "test": "NODE_ENV=testing jest"
  },
  "keywords": [
    "utm",
    "ga",
    "wundertax"
  ],
  "author": "Wundertax",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "atob": ">=2.1.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.12.1",
    "extend": ">=3.0.2",
    "jest": "^22.4.2",
    "jest-localstorage-mock": "^2.2.0",
    "lodash": ">=4.17.11",
    "merge": ">=1.2.1",
    "randomatic": ">=3.0.0",
    "toctoc": "^0.2.3"
  },
  "jest": {
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
